{
  "name": "flowastock",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "server": "json-server --watch db.json --port 3001",
    "start": "concurrently \"npm run server\" \"npm run dev\"",
    "build": "tsr generate && tsc -b && vite build",
    "format": "prettier --write \"src/**/*.{ts,tsx,css,md}\"",
    "preview": "vite preview",
    "lint": "eslint \"src/**/*.{ts,tsx}\" --report-unused-disable-directives --max-warnings 0",
    "prepare": "husky",
    "test:coverage": "vitest run --coverage"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "prettier --check",
      "eslint"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@fontsource-variable/inter": "^5.2.8",
    "@fontsource/inter": "^5.2.8",
    "@hookform/resolvers": "^5.2.2",
    "@mui/icons-material": "^7.3.8",
    "@mui/lab": "^7.0.1-beta.22",
    "@mui/material": "^7.3.8",
    "@mui/x-charts": "^8.27.0",
    "@mui/x-data-grid": "^8.27.1",
    "@mui/x-date-pickers": "^8.27.2",
    "@tanstack/react-router": "^1.161.3",
    "@typescript-eslint/eslint-plugin": "^8.56.0",
    "@typescript-eslint/parser": "^8.56.0",
    "axios": "^1.13.5",
    "dayjs": "^1.11.19",
    "eslint-config-prettier": "^10.1.8",
    "husky": "^9.1.7",
    "json-server": "^1.0.0-beta.9",
    "lint-staged": "^16.2.7",
    "prettier": "^3.8.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-hook-form": "^7.71.2",
    "react-toastify": "^11.0.5",
    "zod": "^4.3.6",
    "zustand": "^5.0.11"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.4.2",
    "@commitlint/config-conventional": "^20.4.2",
    "@eslint/js": "^9.39.1",
    "@tanstack/router-devtools": "^1.161.3",
    "@tanstack/router-plugin": "^1.161.3",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "concurrently": "^9.2.1",
    "eslint": "^9.39.3",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.26",
    "globals": "^16.5.0",
    "jsdom": "^28.1.0",
    "msw": "^2.12.10",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.48.0",
    "vite": "^7.3.1",
    "vitest": "^4.0.18"
  }
}